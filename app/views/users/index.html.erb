

<%= render 'partials/navbar' unless @disable_nav %>

<h1>GymTalk Members</h1>

<ul>
    <% users.each do |user| %>
        <%=link_to user do %>
          <%= image_tag user.avatar.url(:thumb), class: 'thumbnail'%>
            <h4>user.email</h4>            
            <%= link_to user.username, user.email, user_path(user) %> &nbsp;&nbsp;| 
            <%= link_to "Edit", edit_user_path(user) %>
         <% end %>
     <% end %>   
</ul>

